<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_navigation">

    <fragment
        android:id="@+id/navigation_navigation"
        android:name="com.androidspace.fusion.ui.maps.MapFragment"
        android:label="NavigationFragment" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.androidspace.fusion.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_rfid"
        android:name="com.androidspace.fusion.ui.reader.RFIDFragment"
        android:label="@string/title_rfid"
        tools:layout="@layout/fragment_profile" />

</navigation>